    {{ comp.oi.viz.map.svg({ 
        config: {
            geojson: {
                data: geojson.world,
                key: 'ADM0_A3'
            }, 
            projection: {
                name: 'equirectangular'
            },
            data: metrics.partnerships.countries.rows,
            key:'ADM0_A3',
            scale: 'magenta',
            value: 'number_of_partners',
            min: 0,
            width: 600,
            tooltip: 'tooltip',
            columns: [{
                name: 'tooltip',
                template: '{{ geojson.properties.NAME_LONG }}: {{ number_of_partners }} Partner organisation(s)'
            }]
            
        }
      }) | safe }}

{% endcomp %} 


