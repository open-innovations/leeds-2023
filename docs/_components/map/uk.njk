---
css: |
  .uk-map {
    overflow: hidden;
    position: relative;
    left: -0.5px;
    top: -0.5px;
  }
  .uk-map path {
    --fill: rgba(220,220,220,0.5);
    fill: var(--fill);
    fill-rule: evenodd;
    stroke: #3DF2BA;
    stroke-opacity: 1;
    stroke-width: 0.5px;
    stroke-linecap: round;
    stroke-linejoin: round;
  }
---

{%- macro set_data(label) -%}
  {%- set v = (values[label] or 0) -%}
  style="--fill: rgba(61, 242, 0, {{ (v / max) or 0 }});"
  data-value="{{ v / max }}"
  data-label="{{ label }}"
  {% if popup_formatter != undefined -%}
    data-auto-popup="{{ popup_formatter(label, value=values[label], total=total) }}"
  {%- endif %}
{%- endmacro -%}

