<table>
  <thead>
    <tr>
        {% for column in columns %}
        <th id='{{ column.key }}'>
            {{ column.name }}
        </th>
        {% endfor %}
    </tr>
  </thead>
  <tbody>
    {% for row in rows %}
      <tr>
        {% for column in columns %}
          <td>{{ row[column.key] }}</td>
        {% endfor %}
      </tr>
    {% endfor %}
  </tbody>
</table>